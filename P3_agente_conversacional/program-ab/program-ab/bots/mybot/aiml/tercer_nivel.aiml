<category>
<pattern>Tengo algun hueco libre hoy</pattern>
<template>
	<think><set name="topic">hueco_hoy</set></think>
	<think><set var="fecha_hoy"><date jformat="dd/MM/yyyy" /></set></think>
	<think><set var="eventos_hoy"><map name="nombres_eventos"><get var="fecha_hoy"/></map></set></think>
	<think><set name="iterador"><srai>iterate <get var="eventos_hoy"/></srai></set></think>

	<condition var="eventos_hoy">
		<li value="unknown">
			Hoy estas libre
		</li>

		<li>
			<think><set var="i">1</set></think>
			<condition var="i">
				<li value="49">
				</li>

				<li>
					<think><set var="comparar"><srai>compare null with <get name="iterador"/></srai></set></think>
					<condition var="comparar">
						<li value="yes">
							Tienes libre a las <map name="posicion_hora"><get var="i"/></map>
							<think><set name="libre_hoy"><map name="successor"><get var="i"/></map></set></think>
							<think><set var="i">48</set></think>
						</li>
					</condition>

					<think><set name="iterador"><srai>next</srai></set></think>
					<think><set var="i"><map name="successor"><get var="i"/></map>
					</set></think>
					
					<loop/>
				</li>
			</condition>
		</li>
	</condition>
	
</template>
</category>

