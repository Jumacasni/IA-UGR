<category>
<pattern>Tengo el dia libre</pattern>
<template>
	<think><set var="fecha_hoy"><date jformat="dd/MM/yyyy" /></set></think>
	<think><set var="existe"><map name="numero_eventos"><get var="fecha_hoy"/></map></set></think>
	<condition var="existe">
		<li value="unknown">
			¡Sí, puedes aprovechar el día por ahora!
		</li>

		<li>
			No, hoy tienes <get var="existe"/> evento/s
		</li>
	</condition>
</template>
</category>

<category>
<pattern>Cual es mi proximo evento de hoy</pattern>
<template>
	<think><set var="fecha_hoy"><date jformat="dd/MM/yyyy" /></set></think>
	<think><set var="eventos_hoy"><map name="nombres_eventos"><get var="fecha_hoy"/></map></set></think>
	<think><set var="evento_null"><srai>iterate <get var="eventos_hoy"/></srai></set></think>

	<condition var="eventos_hoy">
		<li value="unknown">
			No tienes eventos hoy
		</li>

		<li>
			<think><set var="i">1</set></think>
			<condition var="i">
				<li value="49">
				</li>
				<li>
					<think><set var="comparar"><srai>compare null with <get var="evento_null"/></srai></set></think>
					<condition var="comparar">
						<li value="no">
							Tienes <srai>quitarespacios <get var="evento_null"/></srai> a las <map name="posicion_hora"><get var="i"/></map>
							<think><set var="i">48</set></think>
						</li>
					</condition>


					<think><set var="evento_null"><srai>next</srai></set></think>
					<think><set var="i"><map name="successor"><get var="i"/></map>
					</set></think>
				
					<loop/>
				</li>
			</condition>
		</li>
	</condition>
	
</template>
</category>

